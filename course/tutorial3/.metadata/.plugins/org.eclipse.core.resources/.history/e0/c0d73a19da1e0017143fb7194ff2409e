package tutorial15;

class Person {
	String name;
	int age;
	
	void speakk(){
		System.out.println("My name is " + name);
	}
	
	void calculateYearsToRetiremement() {
		int yearsLeft = 65 - age;
		System.out.println(yearsLeft);
	}
}


public class App {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Person person1 = new Person();
		person1.name = "Joe";
		person1.age = 25;
		
		person1.speakk();
		person1.calculateYearsToRetiremement();
		
		
	}

}
