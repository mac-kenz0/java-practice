package tutorial45;

class Person {
	private int id;
	private String name;

	public Person(int id, String name) {
		this.id = id;
		this.name = name;
	}

	@Override
	public String toString() {
		return "Person [id=" + id + ", name=" + name + "]";
	}
	
	
}

public class App {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
