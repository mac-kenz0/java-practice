package tutorial15;

class Person {
	String name;
	int age;
	
	void speakk(){
		System.out.println("My name is " + name);
	}
	
	int calculateYearsToRetiremement() {
		int yearsLeft = 65 - age;
		return yearsLeft;
	}
	int getAge(){
		return age;
	}
}


public class App {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Person person1 = new Person();
		person1.name = "Joe";
		person1.age = 25;
		
		person1.speakk();
		int years = person1.calculateYearsToRetiremement();
		System.out.println(years);
		
		var agePerson1 = person1.getAge();
		System.out.println(agePerson1);
	}

}
